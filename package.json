{
  "name": "next-tailwind-ts-template",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "format-all": "prettier --ignore-unknown --ignore-path ./.gitignore --write .",
    "format-files": "prettier --ignore-unknown --ignore-path ./.gitignore --write",
    "lint-all": "eslint --cache --ignore-path ./.gitignore --fix .",
    "lint-files": "eslint --cache --ignore-path ./.gitignore --fix",
    "type-check": "tsc --project tsconfig.json --pretty --noEmit",
    "prepare": "husky install"
  },
  "dependencies": {
    "@tailwindcss/forms": "0.4.0",
    "autoprefixer": "10.4.0",
    "next": "12.0.7",
    "next-auth": "4.0.5",
    "postcss": "8.4.4",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "tailwindcss": "3.0.1"
  },
  "devDependencies": {
    "@commitlint/cli": "15.0.0",
    "@commitlint/config-conventional": "15.0.0",
    "@types/node": "16.11.12",
    "@types/react": "17.0.37",
    "@typescript-eslint/eslint-plugin": "5.6.0",
    "@typescript-eslint/parser": "5.6.0",
    "commitizen": "4.2.4",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "8.4.1",
    "eslint-config-next": "12.0.7",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-jsx-a11y": "6.5.1",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-react": "7.27.1",
    "eslint-plugin-react-hooks": "4.3.0",
    "husky": "7.0.4",
    "lint-staged": "12.1.2",
    "prettier": "2.5.1",
    "typescript": "4.5.3"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
